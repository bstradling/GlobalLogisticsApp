<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    tools:context=".ManifestDataActivity">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="0dp">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">

            <EditText
                android:id="@+id/etBillNumber"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:ems="10"
                android:hint="@string/bill_of_lading"
                android:inputType="textPersonName"
                android:minHeight="48dp"
                android:textColor="#000000"
                app:layout_constraintBaseline_toBaselineOf="@+id/tvBillNumber"
                app:layout_constraintEnd_toEndOf="parent" />

            <EditText
                android:id="@+id/etArrival"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:ems="10"
                android:hint="@string/basic_port"
                android:inputType="textPersonName"
                android:minHeight="48dp"
                android:textColor="#000000"
                app:layout_constraintBaseline_toBaselineOf="@+id/tvArrival"
                app:layout_constraintEnd_toEndOf="parent" />

            <TextView
                android:id="@+id/tvVoyageNumber"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="24dp"
                android:layout_marginTop="28dp"
                android:layout_marginEnd="96dp"
                android:text="@string/voyage"
                android:textColor="#000000"
                app:layout_constraintEnd_toStartOf="@+id/etVoyageNumber"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/tvVesselName" />

            <EditText
                android:id="@+id/etDeparture"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:ems="10"
                android:hint="@string/basic_port"
                android:textColor="#000000"
                android:inputType="textPersonName"
                android:minHeight="48dp"
                app:layout_constraintBaseline_toBaselineOf="@+id/tvDeparture"
                app:layout_constraintEnd_toEndOf="parent" />

            <EditText
                android:id="@+id/etConsigneeAddress"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:ems="10"
                android:hint="@string/address"
                android:textColor="#000000"
                android:inputType="textPersonName"
                app:layout_constraintBaseline_toBaselineOf="@+id/tvConsigneeAddress"
                app:layout_constraintEnd_toEndOf="parent" />

