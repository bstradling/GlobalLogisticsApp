                app:layout_constraintBottom_toTopOf="@id/etQuality"
                app:layout_constraintEnd_toEndOf="@+id/etContainerNumber"
                app:layout_constraintTop_toBottomOf="@id/etContainerNumber">

                <EditText
                    android:id="@+id/etDescription"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:ems="10"
                    android:hint="@string/description"
                    android:inputType="textMultiLine"
                    android:textColor="#000000"
                    android:maxLength="2000"
                    android:maxLines="3"
                    android:scrollHorizontally="false"
                    app:layout_constraintBaseline_toBaselineOf="@+id/tvDescription"
                    app:layout_constraintEnd_toEndOf="parent" />

            </ScrollView>

            <EditText
